{
  "name": "casinocoin-libjs",
  "version": "1.0.12",
  "license": "ISC",
  "description": "A JavaScript API for interacting with Casinocoin in Node.js and the browser",
  "files": [
    "dist/npm/*",
    "bin/*",
    "build/*",
    "test/*"
  ],
  "main": "dist/npm/",
  "directories": {
    "test": "test"
  },
  "dependencies": {
    "bignumber.js": "^2.0.3",
    "casinocoin-libjs-address-codec": "^1.0.0",
    "casinocoin-libjs-binary-codec": "^1.0.6",
    "casinocoin-libjs-hashes": "^1.0.2",
    "casinocoin-libjs-keypairs": "^1.0.2",
    "casinocoin-libjs-transactionparser": "^1.0.0",
    "https-proxy-agent": "^2.2.1",
    "jayson": "^2.1.1",
    "jsonschema": "^1.2.4",
    "lodash": "^4.17.11",
    "ws": "^6.1.3"
  },
  "devDependencies": {
    "@types/ejs": "^2.6.1",
    "@types/lodash": "^4.14.120",
    "@types/mocha": "^5.2.5",
    "@types/node": "^10.12.18",
    "@types/ws": "^6.0.1",
    "assert-diff": "^2.0.3",
    "copy-webpack-plugin": "^4.6.0",
    "doctoc": "^1.4.0",
    "ejs": "^2.6.1",
    "json-schema-to-markdown-table": "^0.4.0",
    "mocha": "^5.2.0",
    "mocha-in-sauce": "0.0.1",
    "ts-loader": "^5.3.3",
    "ts-node": "^7.0.1",
    "tslint": "^5.12.1",
    "tslint-webpack-plugin": "^2.0.2",
    "typescript": "^3.2.4",
    "uglifyjs-webpack-plugin": "^2.1.1",
    "webpack": "^4.29.0",
    "webpack-bundle-analyzer": "^3.0.3",
    "webpack-cli": "^3.2.1",
    "webpack-dashboard": "^2.0.0",
    "webpack-dev-server": "^3.1.14",
    "webpack-merge": "^4.2.1"
  },
  "scripts": {
    "doctoc": "doctoc docs/index.md --title '# CasinocoinAPI Reference' --github --maxlevel 2",
    "cp:schemas": "mkdir -p dist/npm/common && cp -r src/common/schemas dist/npm/common/",
    "lint": "tslint -c tsconfig.json src/**/*.ts",
    "clean": "rm -rf dist",
    "start": "webpack-dashboard -- webpack-dev-server --config webpack/dev.config.js --watch",
    "build": "npm run clean && webpack --config webpack/build.config.js"
  },
  "repository": {
    "type": "git",
    "url": "git://github.com/casinocoin/casinocoin-libjs.git"
  },
  "readmeFilename": "README.md",
  "engines": {
    "node": ">=10.15.0"
  }
}
